#!name=Tun Only
#!desc=Enable tun and force 80 port HTTP proxy

[General]
compatibility-mode = 3
force-http-engine-hosts = %APPEND% -<ip-address>, *
tun-excluded-routes = 10.0.0.0/8, 211.99.114.93/32, 140.249.0.0/16, 17.0.0.0/8, 239.255.255.250/32, 121.224.0.0/12, 49.64.0.0/11, 59.82.0.0/16, 36.156.20.0/22, 223.109.128.0/21, 117.128.0.0/10 ,101.226.0.0/16

[Rule]
AND,((NOT,((GEOIP,CN,no-resolve))), (DEST-PORT,443), (PROTOCOL,UDP)),REJECT-NO-DROP

[MITM]
tcp-connection = true

