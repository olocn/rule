#!name=Tun Only
#!desc=Enable tun and force 80 port HTTP proxy

[General]
compatibility-mode = 3
force-http-engine-hosts = %APPEND% -<ip-address>, *
always-real-ip = www.baidu.com,www.abchina.com.cn,yunbusiness.ccb.com,*qq.com, *.gtimg.com, *.taobao.com, *.alicdn.com,*.cn,*.95516.com, *.icloud.com,*.apple.com, *.apple-dns.net
tun-excluded-routes = 10.0.0.0/8, 211.99.114.93/32, 140.249.0.0/16, 17.0.0.0/8, 239.255.255.250/32, 121.224.0.0/12, 49.64.0.0/11, 59.82.0.0/16, 117.128.0.0/10 

[Rule]
AND,((NOT,((GEOIP,CN,no-resolve))), (DEST-PORT,443), (PROTOCOL,UDP)),REJECT-NO-DROP

[MITM]
tcp-connection = true

